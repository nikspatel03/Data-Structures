
public class BstDriver {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		BST obj = new BST();
		
		obj.root = obj.insert(obj.root,15);
		obj.root = obj.insert(obj.root, 10);
		obj.root = obj.insert(obj.root, 25);
		obj.root = obj.insert(obj.root, 3);
		obj.root = obj.insert(obj.root, 33);
		obj.root = obj.insert(obj.root, 37);
		obj.root = obj.insert(obj.root, 5);
		
		if(obj.search(obj.root,37) == true){
			System.out.println("Found");
		}
		else{System.out.println("Not Found");}
		
		
		
		if(obj.search(obj.root,33) == true){
			System.out.println("Found");
		}
		else{
			System.out.println("Not Found");
		}
		
		
		int min = obj.findMin(obj.root);
		int max = obj.findMax(obj.root);
		
		System.out.println(Integer.toString(min));
		System.out.println(Integer.toString(max));
		
	}

}
